# 🔒 Supabase Configuration
# Required for database and auth functionality
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# 🌐 Application URLs
# Used for auth redirects and API endpoints
NEXT_PUBLIC_APP_URL=http://localhost:3000

# 🧪 Testing Environment
# Required for running tests (separate from production)
TEST_SUPABASE_URL=http://localhost:54321
TEST_SUPABASE_ANON_KEY=your_test_anon_key
TEST_DATABASE_URL=postgresql://postgres:postgres@localhost:54321/postgres

# 🚀 CI/CD Configuration
# Required for GitHub Actions deployment
VERCEL_TOKEN=your_vercel_token
VERCEL_ORG_ID=your_org_id
VERCEL_PROJECT_ID=your_project_id

